{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@styles/*": ["./src/styles/*"],
            "@components/*": ["./src/components/*"],
            "@utils/*": ["./src/utils/*"],
            "@images/*": ["./src/assets/images/*"]
        }
    }
}
